<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorators">

	<excludes>
		<pattern>*.tpl.html</pattern>
		<pattern>*template/*.html</pattern>
		<pattern>*.json</pattern>
		<pattern>*.xml</pattern>
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>